.cont{
  padding: 140rpx 36rpx 0;
  box-sizing:border-box;
}
.log-himg{
  width: 160rpx;
  height: 160rpx;
  display: block;
  border-radius: 50%;
  margin: 0 auto;
  overflow: hidden;
  background-color: #ccc;
}
.log-ht{
  position: relative;
  width: 100%;
  display:block;
  margin: 80rpx auto 30rpx;
  font-size: 40rpx;
  line-height: 60rpx;
  font-weight: bold;
  color: #04214d;
  text-align: center;
}
.log-htn{
  font-size: 30rpx;
  font-weight: normal;
  color: #a2acb8;
}
.log-but{
  position: relative;
  padding: 0 28rpx;
  border-radius: 10rpx;
  margin-top: 50rpx;
  font-size: 36rpx;
  line-height: 2.6;
  text-decoration: none;
  text-align: center;
  color: #ffffff;
  background-color: #1AAD19;
  box-sizing: border-box;
  overflow: hidden;
  display: flex;
  justify-content: center;
  align-items:center;
}
.log-but:active{
    background-color: rgb(14, 153, 14);
}
.log-bimg{
  width: 60rpx;
  height: 60rpx;
  margin-right: 10rpx;
  display: inline-block;
}
.log-info{
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  opacity: 0;
}
